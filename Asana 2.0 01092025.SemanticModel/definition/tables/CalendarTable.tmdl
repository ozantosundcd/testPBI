	partition CalendarTable = calculated
		mode: import
		source = ```
				
				ADDCOLUMNS(
				    CALENDAR(
				        MIN('Temps renseignés'[Date de création]), 
				        MAX(MAX('Temps renseignés'[Date de création]), MAX('Taux Occup'[Date d'échéance]))
				    ),
				
				    // --- Dates de base ---
				    "Year", YEAR([Date]),
				    "Month", MONTH([Date]),
				    "Month Name", FORMAT([Date], "MMMM", "fr-FR"),
				    "Month Short Name", FORMAT([Date], "MMM", "fr-FR"),
				    "Year Month", FORMAT([Date], "YYYY-MM"),
				        // --- Ajout : Mois en français avec majuscule + année ---
				    "Year Month Name", 
				        VAR Mois = FORMAT([Date], "[$-fr-FR]MMMM")
				        RETURN UPPER(LEFT(Mois,1)) & MID(Mois,2,LEN(Mois)-1) & " " & YEAR([Date]),
				
				    "Year Month Sort", 
				        YEAR([Date]) * 100 + MONTH([Date]),
				    "Quarter", "Q" & QUARTER([Date]),
				
				    // --- Fiscalité ---
				    "Fiscal Year", IF(MONTH([Date]) >= 4, YEAR([Date]) + 1, YEAR([Date])),
				    "Fiscal Quarter", "Q" & ROUNDUP(MOD(MONTH([Date]) - 3, 12) / 3, 0),
				
				    // --- Semaine ---
				    "Week", WEEKNUM([Date]),
				    "ISO Week", WEEKNUM([Date], 21),
				    "Week Start", [Date] - WEEKDAY([Date], 2) + 1, // Lundi
				    "Week End", [Date] - WEEKDAY([Date], 2) + 7,   // Dimanche
				    "Semaine Affichée", FORMAT([Date] - WEEKDAY([Date], 2) + 1, "dd/MM/yyyy"),
				    "Week of Month", INT( (DAY([Date]) - 1) / 7 ) + 1,
				
				    // --- Jour ---
				    "Day", DAY([Date]),
				    "Day of Week", WEEKDAY([Date], 2),
				    "Day Name", 
				        VAR j = FORMAT([Date], "dddd", "fr-FR")
				        RETURN UPPER(LEFT(j,1)) & MID(j,2,LEN(j)-1),
				    "Day Name Index", WEEKDAY([Date], 2), // Pour le tri de la variable Day name
				    "Mois Actuel", FORMAT(TODAY(), "YYYY-MM"),
				
				
				    "Weekday Short Name", FORMAT([Date], "ddd", "fr-FR"),
				    "Day of Year", DATEDIFF(DATE(YEAR([Date]),1,1), [Date], DAY) + 1,
				
				    // --- Flags ---
				    "Is Weekend", IF(WEEKDAY([Date], 2) > 5, TRUE(), FALSE()),
				    "Is Workday", IF(WEEKDAY([Date], 2) <= 5, TRUE(), FALSE()),
				    "Is Today", IF([Date] = TODAY(), TRUE(), FALSE()),
				    "Is Past Date", IF([Date] < TODAY(), TRUE(), FALSE()),
				    "Is First Day of Month", IF([Date] = EOMONTH([Date], -1) + 1, TRUE(), FALSE()),
				    "Is Last Day of Month", IF([Date] = EOMONTH([Date], 0), TRUE(), FALSE()),
				
				    // --- Mois ---
				    "Month Start", EOMONTH([Date], -1) + 1,
				    "Month End", EOMONTH([Date], 0)
				)
				
				```

